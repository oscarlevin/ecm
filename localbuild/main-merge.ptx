                      <cell>F</cell>
                      <cell>T</cell>
                      <cell>T</cell>
                      <cell>F</cell>
                    </row>
                    <row>
                      <cell right="minor" halign="right">3</cell>
                      <cell>T</cell>
                      <cell>T</cell>
                      <cell>T</cell>
                      <cell>T</cell>
