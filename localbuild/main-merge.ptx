                  <col halign="center"/>
                  <col halign="center"/>
                  <row>
                    <cell>
                      <m>T_n  =</m>
                    </cell>
                    <cell>
                      <m>1</m>
                    </cell>
                    <cell>
                      <m>+</m>
                    </cell>
                    <cell>
                      <m>2</m>
                    </cell>
                    <cell>
                      <m>+ \cdots +</m>
                    </cell>
                    <cell>
                      <m>(n-1)</m>
                    </cell>
                    <cell>
                      <m>+</m>
                    </cell>
                    <cell>n</cell>
                  </row>
                  <row bottom="minor">
                    <cell>
                      <m>+ \quad T_n  =</m>
                    </cell>
                    <cell>
                      <m>n</m>
                    </cell>
                    <cell>
                      <m>+</m>
                    </cell>
                    <cell>
                      <m>(n-1)</m>
                    </cell>
                    <cell>
                      <m>+ \cdots +</m>
                    </cell>
                    <cell>
                      <m>2</m>
                    </cell>
                    <cell>
                      <m>+</m>
                    </cell>
                    <cell>1</cell>
                  </row>
