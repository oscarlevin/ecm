	\draw (A) \va{ R} -- (C) \vr{ B} -- (B) \va{ G} -- (D) \vr{ B} -- (E) \vb{ G} -- (I) \vl{ B} -- (J) \va{ P} -- (F) \vb{ G} -- (G) \vl{ Y} (H) \vl{ R};
	\end{tikzpicture}</latex-image>
