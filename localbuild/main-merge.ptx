                    <row>
                      <cell right="minor" halign="right">4</cell>
                      <cell>F</cell>
                      <cell>F</cell>
                      <cell>F</cell>
                      <cell>F</cell>
                    </row>
                  </tabular>
                </sidebyside>
                <p permid="XtE">
